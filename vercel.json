{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/news_like/:id",
            "dest": "/index.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/news_dislike/:id",
            "dest": "/index.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/login",
            "dest": "/login",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/categories",
            "dest": "/categories",
            "methods": [
                "POST",
                "GET"
            ]
        },
        {
            "src": "/news",
            "dest": "/news",
            "methods": [
                "POST",
                "GET"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}