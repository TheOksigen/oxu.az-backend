{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/categories",
            "dest": "/src/routes/categories.js"
        },
        {
            "src": "/news",
            "dest": "/src/routes/news.js"
        },
        {
            "src": "/login",
            "dest": "/src/routes/login.js"
        }
    ]
}